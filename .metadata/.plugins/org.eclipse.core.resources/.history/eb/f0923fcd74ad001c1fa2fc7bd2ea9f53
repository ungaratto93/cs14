package br.com.caelum.ed;

public class Aluno {

	private String nome;
	
	public String getNome() {
		return this.nome;
	}
	
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public String toString() {
		return this.nome;
	}
	
	public Boolean equals(Object o) {
		Aluno outro = (Aluno)o;
		return this.nome.equals(outro.nome)
	}
}
