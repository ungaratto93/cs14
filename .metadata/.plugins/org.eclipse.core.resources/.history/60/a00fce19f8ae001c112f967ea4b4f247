package br.com.caelum.ed.vetores.testes;

import br.com.caelum.ed.Aluno;
import br.com.caelum.ed.vetores.Vetor;

public class TesteLinearVSConstante {

	public static void main(String[] args) {
		Vetor vetor = new Vetor();
		
		long inicio = System.currentTimeMillis();
		for (int index = 1; index < 100000; index++) {
			Aluno aluno = new Aluno();
			vetor.adicionaDeFormaLinear(aluno);
		}
		long fim = System.currentTimeMillis();
		double tempoLinear = (fim - inicio) / 1000.0;
		
		long inicio = System.currentTimeMillis();
		for (int index = 1; index < 100000; index++) {
			Aluno aluno = new Aluno();
			vetor.adicionaDeFormaLinear(aluno);
		}
		long fim = System.currentTimeMillis();
		double tempoConstante = (fim - inicio) / 1000.0;
		
		System.out.println("[FORMA LINEAR ]Tempo em segundos = " + tempoLinear);
	}
	
}
